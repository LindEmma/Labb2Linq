@model List<Student>

    <h1>Elever som går programmering 1, med lärare</h1> @* All students in prog 1 with teachers *@
<table class="table">
    <thead>
        <tr>
            <th>Elevnamn</th>
            <th>Lärare</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.StudentFirstName @student.StudentLastName</td>
                <td>
                    <ul>
                        @foreach (var enrollment in student.Enrollments)
                        {
                            <li>@enrollment.Teacher.TeacherFirstName @enrollment.Teacher.TeacherLastName (@enrollment.Course.CourseName)</li>
                        }
                    </ul>
                </td>
            </tr>
        }
    </tbody>
</table>